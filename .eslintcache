\n                 authCtx.login(data.data.token,data.data,expirationTime.toISOString())\n                 //  authCtx.login(data.data.token,expirationTime)\n                 history.replace('/');\n            })\n            .catch(\n\n                function (error) {\n                    \n                    try {\n                        if(error.response.data) {\n                            console.log(error.response.data)\n                        } else if (error) {\n                            console.log(error)\n                        }\n                    } catch (error) {\n                      console.log(error)\n                    }\n\n                  }\n            )\n    \n  };\n\n  return (\n    <section className={classes.auth}>\n      <h1>{isLogin ? 'Login' : 'Sign Up'}</h1>\n      <form onSubmit={submitHandler}>\n        <div className={classes.control}>\n          <label htmlFor='email'>Your Email</label>\n          <input type='email' id='email' required ref={emailInputRef} />\n        </div>\n        <div className={classes.control}>\n          <label htmlFor='password'>Your Password</label>\n          <input\n            type='password'\n            id='password'\n            required\n            ref={passwordInputRef}\n          />\n        </div>\n        <div className={classes.actions}>\n          {!isLoading && (\n            <button>{isLogin ? 'Login' : 'Create Account'}</button>\n          )}\n          {isLoading && <p>Sending request...</p>}\n          <button\n            type='button'\n            className={classes.toggle}\n            onClick={switchAuthModeHandler}\n          >\n            {isLogin ? 'Create new account' : 'Login with existing account'}\n          </button>\n        </div>\n      </form>\n    </section>\n  );\n};\n\nexport default AuthForm;\n","C:\\MAMP\\htdocs\\projetos\\projetoAmigosDef\\restauranteFront\\src\\components\\HomePage\\StartingPage\\StartingPageContent.js",[],"C:\\MAMP\\htdocs\\projetos\\pr